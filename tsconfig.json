{
  "compilerOptions": {
    "module": "system",
    "moduleResolution": "node",
    "paths": {
      "*": ["./node_modules/*"]
    },
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "./output.js",
    "sourceMap": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    
},
  "include": ["src/**/*"],
}